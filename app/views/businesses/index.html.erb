 <div id="main">
    <div class="page_margins">
      <div class="page"> 
      <h1>Listing businesses</h1>
      
      <table>
        <tr>
          <th>Id</th>
          <th>User</th>
          <th>Name</th>
          <th>Description</th>
          <th>Zip code</th>
          <th>Address</th>
          <th>Phone</th>
          <th>Operating hours</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      
      <% @businesses.each do |business| %>
        <tr>
          <td><%= business.id %></td>
          <td><%= business.user_id %></td>
          <td><%= business.name %></td>
          <td><%= business.description %></td>
          <td><%= business.zip_code %></td>
          <td><%= business.address %></td>
          <td><%= business.phone %></td>
          <td><%= business.operating_hours %></td>
          <td><%= link_to 'Show', business %></td>
          <td><%= link_to 'Edit', edit_business_path(business) %></td>
          <td><%= link_to 'Destroy', business, :confirm => 'Are you sure?', :method => :delete %></td>
        </tr>
      <% end %>
      </table>
      
      <br />
      
      <%= link_to 'New Business', new_business_path, :class => "button round2" %>
      
      <br />
      <br />
      <br />
      </div>
    </div>
 </div>